          children: [
            const DrawerHeader(child: Text("Menu")),
            ListTile(
              title: const Text("Accueil"),
              onTap: () =&gt; Navigator.pushNamed(context, '/home'),
            ),
          ],
        ),
      ),
      body: isLoading
          ? const Center(child: CircularProgressIndicator())
          : prayerTimes == null
              ? const Center(child: Text("Erreur lors du chargement"))
              : ListView(
                  padding: const EdgeInsets.all(16),
                  children: [
                    // üïå Header image
                    Container(
                      height: 140,
                      width: double.infinity,
                      decoration: const BoxDecoration(
                        image: DecorationImage(
                          image: AssetImage("images/ooo.jpg"), // Your mosque image
                          fit: BoxFit.cover,
                        ),
                      ),
                      alignment: Alignment.center,
                      child: const Text(
                        "ÿ£ŸàŸÇÿßÿ™ ÿßŸÑÿµŸÑÿßÿ© ŸÑŸÑŸäŸàŸÖ",
                        style: TextStyle(
                          fontSize: 20,
                          fontWeight: FontWeight.bold,
                          color: Colors.white,
                          fontFamily: 'Arabic',
                        ),
                      ),
                    ),

                    const SizedBox(height: 20),

                    // üïå Prayer times
                    buildPrayerRow("Fajr", "ÿßŸÑŸÅÿ¨ÿ±", prayerTimes!['Fajr']),
                    buildPrayerRow("Dhuhr", "ÿßŸÑÿ∏Ÿáÿ±", prayerTimes!['Dhuhr']),
                    buildPrayerRow("Asr", "ÿßŸÑÿπÿµÿ±", prayerTimes!['Asr']),
                    buildPrayerRow("Maghrib", "ÿßŸÑŸÖÿ∫ÿ±ÿ®", prayerTimes!['Maghrib']),
                    buildPrayerRow("Isha", "ÿßŸÑÿπÿ¥ÿßÿ°", prayerTimes!['Isha']),

                    const SizedBox(height: 20),

                    // üåÖ Sunrise / Sunset
                    buildSunCard("Lev√©e", "ÿßŸÑÿ¥ÿ±ŸàŸÇ", "images/ttt.png", prayerTimes!['Sunrise']),
                    buildSunCard("Coucher", "ÿßŸÑÿ∫ÿ±Ÿàÿ®", "images/rrr.png", prayerTimes!['Sunset']),
                  ],
                ),
    );
  }
}
